{
  "parser": "babel-eslint",
  "extends": ["plugin:react/recommended"],
  "rules": {
    "no-console":"warn",
    "semi-spacing":"error",
    "comma-dangle":"error",
    "semi":"error",
    "no-trailing-spaces":"error",
    "no-multi-spaces":"error",
    "space-before-blocks":"error",
    "comma-spacing":"error",
    "key-spacing":"error",
    "no-empty": "error",
    "no-extra-semi": "error",
    "no-sparse-arrays": "error",
    "array-bracket-newline": "error",
    "camelcase":"error",
    "eol-last":"error",
    "indent": "error",
    "jsx-quotes":"error",
    "prefer-destructuring": "error"
  }
}